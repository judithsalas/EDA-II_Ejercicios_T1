def bucle_for():
    suma_total = 0
    for numero in lista_enteros:
        suma_total += numero
    print(f"La suma de todos los elementos de la lista es: {suma_total}")


def suma_funcion():
    suma_total = sum(lista_enteros)
    print(f"La suma de todos los elementos de la lista es: {suma_total}")
